<% content_for :title, title_with_error_prefix(title, @survey.errors.any?) %>
<% content_for :before_content, govuk_back_link_to(previous_url) %>

<%= form_with model: @survey, url: url, method: :post do |f| %>

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

      <h2 class="govuk-heading-xl">
        <%= title %>
      </h2>

      <%= render 'form', f: f %>
    </div>
  </div>
<% end %>
